#!/bin/sh

case "$1" in
    configure)  
		echo "unload old udlfb-module"
		modprobe -r udlfb
		echo "load new udlfb-module"
		modprobe udlfb
		signal-event activate-displaylink
	;;

esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

exit 0

#DEBHELPER#
